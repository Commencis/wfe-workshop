{
  "name": "commencis-wfe-workshop",
  "version": "1.0.0",
  "build": "0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.18"
  },
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate:gql": "graphql-codegen --require dotenv/config --config codegen.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint:check:types": "tsc --noEmit",
    "lint:check:js": "eslint .",
    "lint:fix:js": "eslint . --fix",
    "lint:check:css": "stylelint **/*.{css,scss}",
    "lint:fix:css": "stylelint **/*.{css,scss} --fix",
    "lint:check:format": "prettier \"**/*.{json,md}\" --check",
    "lint:fix:format": "prettier \"**/*.{json,md}\" --write",
    "lint": "pnpm run \"/^lint:check:.*/\"",
    "lint:fix": "pnpm run \"/^lint:fix:.*/\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "changeset": "changeset",
    "version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci:review": "pnpm run lint && pnpm run test",
    "generate:worker": "pnpm exec msw init ./public --save",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@apollo/client": "3.13.4",
    "@hookform/resolvers": "4.1.3",
    "@reduxjs/toolkit": "2.6.1",
    "axios": "1.8.3",
    "clsx": "2.1.1",
    "graphql": "16.10.0",
    "i18next": "24.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-i18next": "15.4.1",
    "react-redux": "9.2.0",
    "react-router": "7.3.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@commencis/commitlint-config": "1.1.3",
    "@commencis/eslint-config": "1.7.0",
    "@commencis/eslint-plugin": "1.4.0",
    "@commencis/lint-staged-config": "0.1.0",
    "@commencis/prettier-config": "2.0.0",
    "@commencis/stylelint-config": "2.0.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@storybook/addon-a11y": "8.6.4",
    "@storybook/addon-actions": "8.6.4",
    "@storybook/addon-docs": "8.6.4",
    "@storybook/addon-essentials": "8.6.4",
    "@storybook/addon-interactions": "8.6.4",
    "@storybook/addon-links": "8.6.4",
    "@storybook/blocks": "8.6.4",
    "@storybook/react": "8.6.4",
    "@storybook/react-vite": "8.6.4",
    "@storybook/test": "8.6.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react-swc": "3.8.0",
    "@vitest/coverage-v8": "3.0.8",
    "commitlint": "19.8.0",
    "dotenv": "16.4.7",
    "eslint": "9.22.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.5.0",
    "msw": "2.7.3",
    "prettier": "3.5.3",
    "sass-embedded": "1.85.1",
    "storybook": "8.6.4",
    "stylelint": "16.16.0",
    "typescript": "5.8.2",
    "vite": "6.2.2",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "msw"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
